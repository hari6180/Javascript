
public class SinglyLinkedList {

	class Node {
		int data;
		Node next = null;

		Node(int d) {
			this.data = d;
		}

		void append(int d) {
			Node end = new Node(d);
			Node n = this;

			while (n.next != null) {
				;
				n = n.next;
			}

			n.next = end;
		}

	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
